{
	"version": "0.1",
	"manifest_version":2,
	"name": "dos chromy theme",
	"description": "it makes your chrome look like a dos window",
	"theme": {
		"images" : {
			"theme_frame" : "images/dos_menue_gray.png",
			"theme_tab_background" : "images/dos_menue_gray.png",
			"theme_button_background" : "images/dos_option_menue.png",
			"theme_toolbar" : "images/dos_option_menue_bigger.png",
			"theme_frame_overlay" : "images/msdoslogo_small.png",
			"theme_ntp_background" : "images/dos_main_area.png"
		},
		"colors" : {
			"frame" : [167,167,167],
			"tab_text" : [204,255,130],
			"bookmark_text" : [211,255,255],
			"ntp_text" : [204,255,130]
		},
		"tints" : {
			"buttons" : [0.5, 0, 1]
		},
		"properties" : {
			"ntp_logo_alternate" : 1,
			"ntp_background_alignment" : "left top",
			"ntp_background_repeat" : "repeat"
		}
	}
}
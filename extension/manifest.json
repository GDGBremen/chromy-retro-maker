{
	"version": "0.1",
	"name": "dos chromy extension",
	"description": "it makes all the websites your visiting, looking like dos",
	"manifest_version": 2,
	"icons": { "128": "icon128.png" },
	
	"content_scripts": [ {
		"css": ["retro.css"],
		"js": ["jquery.js", "main.js"],
		"matches": [ "http://*/*", "https://*/*"],
        "all_frames": true
	}],
	"web_accessible_resources": ["overlay.png", "retro.vs", "retro.fs", "ohmy.jpg", "cursor.png"]
}